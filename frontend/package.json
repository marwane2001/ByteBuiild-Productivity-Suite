{
  "name": "startup-suite",
  "version": "0.1.0",
  "private": true,
  "main": "src/main/electron.ts",
  "homepage": "./",
  "dependencies": {
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-plotly.js": "^2.6.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently -k \"npm run start:react\" \"npm run start:electron\"",
    "start:react": "react-scripts start",
    "start:electron": "npm run build:electron && electron dist/main/electron.js",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build": "react-scripts build && npm run build:electron"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron": "^1.4.38",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.2",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^19.1.2",
    "concurrently": "^9.1.2",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.startupsuite.app",
    "productName": "Startup Suite",
    "files": [
      "build/**/*",
      "src/main/**/*"
    ],
    "directories": {
      "output": "dist"
    }
  }
}
